FLAGS=-I ../ -Wshadow -Winit-self -Wredundant-decls -Wcast-align -Wundef -Wfloat-equal -Winline -Wunreachable-code -Wmissing-declarations -Wmissing-include-dirs -Wswitch-enum -Wswitch-default -Weffc++ -Wmain -Wextra -Wall -g -pipe -fexceptions -Wcast-qual -Wconversion -Wctor-dtor-privacy -Wempty-body -Wformat-security -Wformat=2 -Wignored-qualifiers -Wlogical-op -Wno-missing-field-initializers -Wnon-virtual-dtor -Woverloaded-virtual -Wpointer-arith -Wsign-promo -Wstack-usage=8192 -Wstrict-aliasing -Wstrict-null-sentinel -Wtype-limits -Wwrite-strings -Werror=vla -D_DEBUG -D_EJUDGE_CLIENT_SIDE
NODEBUG = -g
all: asm

asm: ..\MC_Onegin\read_poem_from_file_functions.o ..\MC_Onegin\clean_memory_functions.o ..\STACK\stack_functions.o ..\MC_Onegin\text_functions.o assembler_main.o ..\COMMON\common_functions.o ..\STACK\dump_functions.o assembler_functions.o ..\SPU\SPU_functions.o
	g++ $(FLAGS) ..\MC_Onegin\read_poem_from_file_functions.o ..\MC_Onegin\clean_memory_functions.o ..\STACK\stack_functions.o ..\MC_Onegin\text_functions.o assembler_main.o ..\COMMON\common_functions.o ..\STACK\dump_functions.o assembler_functions.o ..\SPU\SPU_functions.o -o asm

..\MC_Onegin\text_functions.o: ..\MC_Onegin\text_functions.cpp
	g++ $(FLAGS) -c ..\MC_Onegin\text_functions.cpp

..\MC_Onegin\read_poem_from_file_functions.o: ..\MC_Onegin\read_poem_from_file_functions.cpp
	g++ $(FLAGS) -c ..\MC_Onegin\read_poem_from_file_functions.cpp

..\MC_Onegin\clean_memory_functions.o: ..\MC_Onegin\clean_memory_functions.cpp
	g++ $(FLAGS) -c ..\MC_Onegin\clean_memory_functions.cpp

assembler_main.o: assembler_main.cpp
	g++ $(FLAGS) -c assembler_main.cpp

..\STACK\stack_functions.o: ..\STACK\stack_functions.cpp
	g++ $(FLAGS) -c ..\STACK\stack_functions.cpp

..\SPU\SPU_functions.o: ..\SPU\SPU_functions.cpp
	g++ $(FLAGS) -c ..\SPU\SPU_functions.cpp -o ..\SPU\SPU_functions.o

..\COMMON\common_functions.o: ..\COMMON\common_functions.cpp
	g++ $(FLAGS) -c ..\COMMON\common_functions.cpp -o ..\COMMON\common_functions.o

..\STACK\dump_functions.o: ..\STACK\dump_functions.cpp
	g++ $(FLAGS) -c ..\STACK\dump_functions.cpp

assembler_functions.o: assembler_functions.cpp
	g++ $(FLAGS) -c assembler_functions.cpp

clean:
	rm -rf *.o asm
